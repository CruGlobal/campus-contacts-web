<title><%= @title || (mhub? ? 'Take a Survey' : ENV['SITE_TITLE']) %></title>
<link rel="icon" type="image/png" href="<%= image_path("favicon.png") %>">
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0"/>
<meta name="apple-itunes-app" content="app-id=447869440">
<link rel="manifest" href="<%= asset_path("manifest.json") %>">
<%= csrf_meta_tag %>

<%= stylesheet_link_tag 'application' %>
<%= stylesheet_link_tag 'no_left_sidebar' if no_left_sidebar %>

<%= stylesheet_link_tag 'https://ajax.googleapis.com/ajax/libs/jqueryui/1.7.2/themes/redmond/jquery-ui.css' %>
<%= stylesheet_link_tag 'https://fonts.googleapis.com/css?family=Montserrat:400,700|Titillium+Web:400,700' %>

<% if Rails.env.development? %>
  <%= javascript_include_tag 'jquery-1.7.2.min.js' %>
  <%= javascript_include_tag 'jquery-ui-1.8.12.js' %>
<% else %>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js" type="text/javascript"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.12/jquery-ui.js" type="text/javascript"></script>
<% end %>


<%= javascript_include_tag 'application' %>
<%#= stylesheet_link_tag 'smoothness/jquery-ui-1.8.7.custom' %>
<% unless Rails.env.development? %>
  <%= render 'adobe_analytics_head' %>
<% end %>
