<title><%= @title || APP_CONFIG['site_title'] %></title>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.min.js" type="text/javascript"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.12/jquery-ui.js" type="text/javascript"></script>

<%= stylesheet_link_tag '/stylesheets/smoothness/jquery-ui-1.8.7.custom.css' %>
<%= stylesheet_link_tag 'application' %>
<%#= stylesheet_link_tag 'jquery.qtip.css' %>
<%#= stylesheet_link_tag 'superfish.css' %>
<%= javascript_include_tag 'application' %>
<%#= javascript_include_tag 'jquery.qtip.js' %>
<%#= javascript_include_tag 'superfish.js' %>
<%#= stylesheet_link_tag 'smoothness/jquery-ui-1.8.7.custom' %>
<%= csrf_meta_tag %>
<script type="text/javascript" charset="utf-8">
  $.params = <%= raw params.dup.keep_if {|k, v| v.is_a?(String) || v.is_a?(Numeric) || v.is_a?(Enumerable)}.to_json %>;
</script>

<script type="text/javascript">

  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-325725-21']);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

</script>