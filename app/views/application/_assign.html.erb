<div id="assign_search" style="display:none" class="custom_mh_popup_box">
  <div class="ui-widget-overlay pop_box_overlay_container"></div>
  <div class="pop_box_container">
    <div class="mh_popup_box assign_popup_box" class="ui-dialog ui-widget ui-widget-content ui-corner-all ui-draggable" tabindex="-1" permission="dialog" aria-labelledby="ui-dialog-title-alert_dialog">
    	<div class="mh_popup_box_top"><span class="mh_popup_heading">Assign</span></div>
      <div id="assign_search_loader" style="display:none;">
        <%= image_tag 'loader.gif'%><br/>Loading
      </div>
      <div class="mh_popup_box_content" id="mh_popup_box_content">
        <%= render partial: 'application/assign_search'%>
      </div>
    	<div class="assign_save" id="assign_save" style="">
    		<button class="red_accent assign_button" id="assign_popup_save_button">Assign</button>
    		<button class="large_lightgray" id="assign_popup_cancel_button">Cancel</button>
    	</div>
    </div>
  </div>
</div>
