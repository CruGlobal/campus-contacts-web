<div class="titlebar titlebarmod">
  <div id='global_loader'>
    <%= image_tag 'loader.gif'%>
  </div>
  <h1>
    <%= t('.title', survey: @survey.title) %>
  </h1>
  <div id="ac_button_bar" class="mass_entry_buttons">
  	<%= render "actions" %>
  </div>
</div>

<div id="ac_contact_spread">
  <%- questions = Array.new %>
  <div class='loader-space fetching-loader hidden'>
    <%= image_tag 'loader.gif'%>
    <br/>
    <%= t('.loading') %>
  </div>
  <div class='loader-space saving-loader hidden'>
    <%= image_tag 'loader.gif'%>
    <br/>
    <%= t('.saving') %>
  </div>
  <div id="mass_entry_table" data-survey-id="<%= @survey.id %>">
  </div>
</div>

<!-- Batch Change Info Dialog -->
<%= render 'dialog_new_label' %>