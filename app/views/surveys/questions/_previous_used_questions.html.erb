<div id="previously_used" style="display:none;" class="custom_mh_popup_box">
  <div class="ui-widget-overlay pop_box_overlay_container">&nbsp;</div>
  <div class="pop_box_container">
    <div class="mh_popup_box">
    	<div class="mh_popup_box_top">
        <span class="mh_popup_heading" id="member_search_title">
          <%= t('surveys.questions.index.add_prev_question') %>
        </span>
      </div>
      <div class="mh_popup_box_content">
        <div class="mh_popup_box_scroller">
          <table class="listing contact_listing yourquestions" cellspacing="0" cellpadding="0">
            <tr id="table_header">
              <th>
                <%= t('surveys.questions.index.predefined') %> <%= tip(t('surveys.questions.index.predefined_help')) %>
              </th>
              <th>&nbsp;</th>
            </tr>
            <tbody id="predefined_questions">
              <% @predefined_questions.each do |question| %>
              	<% next if ['first_name','last_name','phone_number'].include?(question.attribute_name) %>
                <%= render 'predefined_question', question: question %>
              <% end %>
            </tbody>
          </table>

          <table class="listing contact_listing yourquestions" cellspacing="0" cellpadding="0">
            <tr id="table_header">
              <th>
                <%= t('surveys.questions.index.previously_used') %> <%#= tip(t('questions.previously_used_help')) %>
              </th>
              <th>&nbsp;</th>
            </tr>
            <tbody id="other_questions">
              <% @other_questions.each do |question| %>
                <%= render 'predefined_question', question: question %>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    	<div class="mh_popup_buttons">
    		<button class="large_lightgray mh_popup_cancel_button" id="">Cancel</button>
    	</div>
    </div>
  </div>
</div>