$('#create_question_form').html('');
$('#question_form').hide();
$('#question_form').html('<%= j(render('form', question: @question)) %>');
$('#loading').hide();
$('#question_form').slideDown();
$.fn.tip()

//Instantiate search if has data for Automatic Assignment of Contacts
$(document).ready(function() {
  <% name = "" %>
  <% id = "" %>
  <% if @auto_assign.present? && @auto_assign.extra_parameters.present? %>
    <% name = @auto_assign.extra_parameters['name'] %>
    <% id = @auto_assign.extra_parameters['id'] %>
  <% end %>
	autoassign_search("<%= id %>", "<%= name %>");
});