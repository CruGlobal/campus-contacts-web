<div id="transfer_survey_div" style="display:none;" class="custom_mh_popup_box">
  <div class="ui-widget-overlay pop_box_overlay_container">&nbsp;</div>
  <div class="pop_box_container">
    <div class="mh_popup_box">
    	<div class="mh_popup_box_top">
        <span class="mh_popup_heading">
          <%= t("surveys.index_admin.copy_survey") %>
        </span>
      </div>
      <div class="mh_popup_box_content">
        <div class="fancy" id="transfer_survey_guide">
          You are about to copy the survey "<span id="survey_name"></span>" to another organization. Please select an organization you wish to have the same survey.
        </div>
        <div class="mh_popup_box_scroller">
          <div id="transfer_survey_processing">
            <%= image_tag 'loader.gif'%><br/>Copying Survey to <span id="org_name"></span><br/>Please wait...
          </div>
          <div id="transfer_survey_content">
            <div id="other_orgs_filter">
              <%= text_field_tag "filter", "keyword", id: "other_orgs_filter_keyword", value: "", placeholder: "Type to search organization", class: "ui-autocomplete-input" %>
            </div>
            <div id="other_orgs_list">
            </div>
          </div>
          <div id="transfer_survey_error">
          </div>
        </div>
      </div>
    	<div class="mh_popup_buttons">
    		<button class="large_lightgray mh_popup_cancel_button" id="">Cancel</button>
    	</div>
    </div>
  </div>
</div>