<% if params[:from_add_member_screen] == "true" %>
  <% unless params[:add_another] == 'true' %>
    $("#member_search").dialog('close');
  <% end %>
  <% @persons.each do |person| %>
    row = '<%= j(render('groups/person', person: person, group_membership: @group_membership)) %>'
    if($('#<%= dom_id(person) %>')[0]) {
      $('#<%= dom_id(person) %>').replaceWith(row)
    } else {
      $('#group_table').append(row)
    }
  <% end %>
<% end %>
