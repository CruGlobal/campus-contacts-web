<% if params[:from_add_member_screen] == "false" %>
  <% unless params[:add_another] == 'true' %>
    $("#member_search").dialog('close');
  <% end %>
  <% @persons.each do |person| %>
    row = '<%= j(render('groups/person', person: person, group_membership: @group_membership)) %>'
    if($('#<%= dom_id(person) %>')[0]) {
      $('#<%= dom_id(person) %>').replaceWith(row)
    } else {
      $('#group_table').append(row)
    }
  <% end %>
<% else %>
   <% unless params[:add_another] == 'true' %>
     $("#member_search").dialog('close');
   <% end %>
   row = '<%= j(render('groups/person', person: @persons, group_membership: @group_membership)) %>'
   if($('#<%= dom_id(@persons) %>')[0]) {
     $('#<%= dom_id(@persons) %>').replaceWith(row)
   } else {
     $('#group_table').append(row)
   }
<% end %>
