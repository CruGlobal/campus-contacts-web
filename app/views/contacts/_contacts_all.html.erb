<tr id="all">
	<td colspan="99" id="all_contacts_select_all_option">
		<span id="all_text">
      <% @filtered_people.each do |person| %>
    		<%= render "contacts/contact_checkbox", person: person %>
      <% end %>

			<%=raw t('people.all.page_selected_html', count: @people.length) %>
			<% if @all_people.length > @people.length %>
			  <a href="#" id="fetch_all_contacts_link">
			    <%=raw t('people.all.select_all_html', count: @all_people.length) %>
			  </a>
			<% end %>
		</span>

		<span id="all_selected_text" style="display:none">
			<%=raw t('people.all.all_selected_html', count: @all_people.length) %>
		</span>
	</td>
</tr>
