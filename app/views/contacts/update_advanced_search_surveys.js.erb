<% @filtered_surveys.order(:title).each do |survey| %>
  if($(".side-search-survey#side-search-survey-<%= survey.id %>").size() == 0){
    $(".side-search .side-search-surveys").append("<%= escape_javascript(render partial: 'search/survey', locals: {survey: survey}) %>")
  }
<% end %>

<% @remove_survey_ids.each do |survey_id| %>
  $(".side-search-survey#side-search-survey-<%= survey_id %>").remove()
<% end %>

$.fn.filterLoader('hide')