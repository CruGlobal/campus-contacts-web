stamp = $("#ac_sidebar #sidebar_lists").data('stamp')
clear = "<%= params[:filters] %>" == "clear"
if(stamp == '' || stamp == "<%= params[:search_stamp] %>" || clear || <%= params[:paginate] == 'true' %>){
  if(clear){
    $('.save-search-option').hide()
    $("#ac_sidebar #sidebar_lists").html("<%= escape_javascript(render partial: 'search/sidebar') %>")
  }

  $.fn.handleSearchOptions()

  if(<%= session[:filters].present? && @people.length > 0 && !@saved_contact_search.full_path.present? %>){
    $('.save-search-option').show()
  }else{
    $('.save-search-option').hide()
  }

  $.fn.filterLoader('force')
  $("#ac_pagination_top").html("<%= escape_javascript(render partial: 'contacts/pagination') %>")
  $("#ac_contact_list").html("<%= escape_javascript(render partial: 'contacts/contact_list') %>")
  $(".pagination_space #per_page").attr("disabled", false)
  $(".scroll_table").doubleScroll()
}