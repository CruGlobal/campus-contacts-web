<% if params[:assign_to_me] == 'true' %>
	$('#contacts_table').prepend("<%= j(render('contacts/my_contact', :person => @person)) %>")
<% else %>
	$('#contacts_table').prepend("<%= j(render('contacts/contact', :person => @person)) %>")
<% end %>
$('#add_contact_div').dialog('close')
$('#<%= dom_id(@person) %>').effect('highlight')
$('.no_contacts').hide()
$('#add_contact_div form')[0].reset()
