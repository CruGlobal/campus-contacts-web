<div id='all_feeds_container'>
  <% @all_feeds.each do |feed| %>
    <% case feed.class.to_s %>
    <% when "Interaction" %>
      <div class='all_feed_container' id='all_feed_container_<%= feed.id %>'>
        <%= render partial: 'interactions/interaction_feed', locals: {interaction: feed} %>
      </div>
    <% when "AnswerSheet" %>
      <%= render partial: 'interactions/survey_feed', locals: {answer_sheet: feed} %>
    <% end %>
  <% end %>
</div>
<div id='all_feed_paging'>
  <% if @all_feeds.last != @last_all_feeds %>
    <%= link_to(raw("<button class='action_gray'>Load More</button>"), '', id: 'all_feeds_paging_link', 'data-next-page' => @all_feeds_page + 1, 'data-person-id' => @person.id) %>
  <% end %>
</div>