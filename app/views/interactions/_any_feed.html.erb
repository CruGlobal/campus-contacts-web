<% case feed.class.to_s %>
<% when "Interaction" %>
  <% if feed.receiver.present? %>
    <div class='all_feed_container' id='all_feed_container_<%= feed.id %>'>
      <%= render partial: 'interactions/interaction_feed', locals: {interaction: feed} %>
    </div>
  <% end %>
<% when "AnswerSheet" %>
  <%= render partial: 'interactions/survey_feed', locals: {answer_sheet: feed} %>
<% when "OrganizationalLabel" %>
  <%= render partial: 'interactions/label_feed', locals: {org_label: feed} %>
<% when "Message" %>
  <%= render partial: 'interactions/message_feed', locals: {message: feed} %>
<% end %>