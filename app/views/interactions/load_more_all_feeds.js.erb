$.toggleLoader('all_feed_paging','')
<% @all_feeds.each do |feed| %>
  $('#all_feeds_container').append("<div class='all_feed_container new' id='all_feed_container_<%= feed.id %>'><%= escape_javascript(render partial: 'interactions/interaction_feed', locals: {interaction: feed}) %></div>")
  $('.all_feed_container.new').each(function(){
    $(this).hide().fadeIn('slow')
    $(this).removeClass('new')
  })
<% end %>
<% if @all_feeds.last != @last_all_feeds %>
  $('#all_feed_paging').html("<%= escape_javascript(link_to(raw('<button class=action_gray>Load More</button>'), '', id: 'all_feeds_paging_link', 'data-next-id' => @all_feeds_page + 1, 'data-person-id' => @person.id)) %>")
<% end %>