$("#leader_search").dialog('close');
var leaders = $('#leaders');
leaders.prepend('<%= j(render('contacts/leader', person: @person)) %>')
$.fn.activateLeaderDroppable();
leaders.css({height: 'auto'})
leaders.attr("data-original-height", leaders.height())
if (leaders.hasClass('showall')) {
  // leaders.attr('data-height', leaders.height())
	leaders.animate({height: leaders.attr("data-original-height")})
}	else {
	leaders.animate({height: leaders.attr("data-height")})
}
