$('#<%= dom_id(@parent) %> ul').prepend('<%= j(render('org', org: @organization)) %>')
<% unless params[:add_another] == 'true' %>
	$('#new_org').dialog('destroy')
<% end %>
$('[data-id=<%= @organization.id %>]').effect('highlight')
$('.org_big_add').closest('li').hide()
$('#new_org').html('<%= j(render('add_org', organization: @parent.children.new)) %>')
