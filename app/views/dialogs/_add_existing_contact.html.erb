<div id="existing_contact_dialog" style="display:none;" class="custom_mh_popup_box">
  <div class="ui-widget-overlay pop_box_overlay_container"></div>
  <div class="pop_box_container">
    <div class="mh_popup_box permissions_popup_box">
    	<div class="mh_popup_box_top">
        <span class="mh_popup_heading"><%= t('application.add_contact.existing_contact') %></span>
      </div>
    	<div id="avatar_form_container">
        <div class="fancy"><%= t('application.add_contact.existing_person_email') %></div>
        <div id="existing_contacts_loader">
          <%= image_tag 'loader.gif'%><br/>
          <%= t('application.add_contact.loading_description') %>
        </div>
        <div id="existing_contacts_container">
          <table id="existing_contacts_table" class="listing" cellpadding="0" cellspacing="0" style="display:none">
            <thead>
            	<th><%= t('contacts.all_contacts.first_name') %></th>
            	<th><%= t('contacts.all_contacts.last_name') %></th>
              <th>&nbsp;</th>
            </thead>
            <tbody id="existing_contact_content">
            </tbody>
          </table>
        </div>
    	</div>
    	<div class="existing_contact_buttons_div">
    		<button class="large_lightgray mh_popup_cancel_button" id="">Cancel</button>
    	</div>
    </div>
  </div>
</div>