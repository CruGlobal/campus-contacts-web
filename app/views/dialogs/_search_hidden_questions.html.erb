<div id="search_hidden_questions_div" style="display:none;" class="custom_mh_popup_box">
  <div class="ui-widget-overlay" style="position: fixed; width: 100%; height: 100%; z-index: 999; "></div>
  <div class="pop_box_container">
    <div class="mh_popup_box" class="ui-dialog ui-widget ui-widget-content ui-corner-all ui-draggable" tabindex="-1" permission="dialog" aria-labelledby="ui-dialog-title-alert_dialog" id="search_hidden_questions_box">
    	<div class="mh_popup_box_top">
        <span class="mh_popup_heading">
          Show Hidden Question
        </span>
      </div>
      <div class="mh_popup_box_content">
        <div id="search_hidden_questions_fetcher">
          <%= image_tag 'loader.gif'%><br/>Fetching Questions...
        </div>
        <div id="search_hidden_questions_loader">
          <%= image_tag 'loader.gif'%><br/>Adding Question
        </div>
        <div id="search_hidden_questions_content">
        </div>
      </div>
    	<div class="mh_popup_buttons" id="search_hidden_questions_buttons">
    		<button class="large_lightgray mh_popup_cancel_button" id="">Cancel</button>
    	</div>
    </div>
  </div>
</div>