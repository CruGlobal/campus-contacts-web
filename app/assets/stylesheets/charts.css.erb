div#snapshot_radio_button_all{
  float:left;
}

div#snapshot_radio_button_custom{
  float:right;
}
*/
div#snapshot_movements_submit, div#goal_description, div#trend_movements_submit {
  width:100%;
}

div#snapshot_movements_submit input, div#trend_movements_submit input{
  width:140px;
  margin:12px auto 0;
}

div#goal_description input {
  width: 140px;
  margin-bottom: 12px;
}

div#goal_description input#organizational_goal_start_value, div#goal_description input#organizational_goal_end_value {
  width: 100px;
}

button#uncheck_all_movements, button#trend_uncheck_all_movements{
  width: 140px;
  margin: 12px auto;
}

div#goal_description input#save_goal_button, button#cancel_edit_goal_button {
  width: 140px;
}

div#trend_date_select {
  margin: 0px 8px;
}

div#trend_date_select input {
  width: 140px;
}

div#trend_fields {
  width: 100%;
}

div.trend_fields_row, div#trend_compare {
  display: table;
  margin: 0 auto;
}

div.trend_fields_row select {
  margin: 20px;
}


input[type="submit"].large_gray {
    /*--Border--*/
    border: 1px solid #333333;
    -webkit-border-radius: 4px;
    /* Android ≤ 1.6, iOS 1-3.2, Safari 3-4 */
    border-radius: 4px;
    /* Android 2.1+, Chrome, Firefox 4+, IE 9+, iOS 4+, Opera 10.50+, Safari 5+ */
    /*--Gradient--*/
    background: #808284;
    /* Old browsers */
    background: -moz-linear-gradient(top, silver 0%, #858789 50%, #808284 50%, #808284 100%);
    /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, silver), color-stop(50%, #858789), color-stop(50%, #808284), color-stop(100%, #808284));
    /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, silver 0%, #858789 50%, #808284 50%, #808284 100%);
    /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, silver 0%, #858789 50%, #808284 50%, #808284 100%);
    /* Opera 11.10+ */
    background: -ms-linear-gradient(top, silver 0%, #858789 50%, #808284 50%, #808284 100%);
    /* IE10+ */
    background: linear-gradient(to bottom, silver 0%, #858789 50%, #808284 50%, #808284 100%);
    /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#c0c0c0', endColorstr='#808284',GradientType=0 );
    /* IE6-9 */
    /*--Font--*/
    font-family: Arial;
    font-size: 12px;
    font-weight: bold;
    text-align: center;
    color: #FFF;
    /*line-height:24px;*/
    text-shadow: 0px -1px #333333;
    /*--Other--*/
    height: 32px;
    min-width: 85px;
    padding: 0 9px;
    display: block;
    cursor: pointer;
  }


input[type="submit"].large_gray:hover {
    background: #808284;
    border-color:#333333;
    text-shadow: 0px -1px #333333;
}

input[type="submit"].large_gray a {
    display:block;
    line-height:30px;
}

