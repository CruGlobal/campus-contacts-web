div#clear {
  clear: both;
}

div#snap_container1, div#snap_container2, div#snap_container3, div#snap_numbers {
  position: relative;
  top: 0px;
  right: 0px;
  width: 50%;
  height: 300px;
  margin: 0 auto;
  float: left;
}

div#snap_gospel_exp, div#snap_laborers, div#snap_on_mission {
  width: 50%;
  height: 75px;
  float: left;
}

div#highcharts-0 defs+rect, div#highcharts-2 defs+rect, div#highcharts-4 defs+rect {
  fill:none;
}

svg>text[text-anchor=end] tspan{
  display:none;
}

div#snap_spinner, div#goal_spinner, div#goal_box_spinner, div#trend_spinner {
  width: 100%;
  display:none;
}

div#rest {
  background:none;
}

div#rest #main, div#gandalfthewhite {
  border:none;
}

div#rest #main {
  padding:0 20px;
}

div#goal_select_org, div#goal_select_criteria {
  margin-left:10px;
}

div#goal_description form{

}

div#goal_description {
  margin:12px 0;
}

div#goal_box h3, div#goal_box h4{
  margin:4px 0;
}

div.fieldWithErrors div.errorExplanation ul li {
  color: rgb(204, 0, 0);
}

button#edit_goal_button, button#cancel_edit_goal_button, input[type="submit"]#save_goal_button {
  margin:12px auto;
}

div#snapshot_movements, div#trend_movements {
  background-color:#ffffff;
  padding:8px 12px 8px 8px;
  max-height:300px;
  overflow-x:wrap;
  overflow-y:scroll;
  border:1px solid #c0c0c0;
  margin:0 8px;
}

div#snap_movements_data h2 {
  float: left;
  margin-top: 10px;
}

div#snap_movements_data div.help_bubble_keyword {
  float: left;
  margin-top: 10px;
  margin-left: 5px;
}

div#snapshot_sidebar, div#goal_box, div#trend_sidebar {
  background-color:#f6f6f6;
  /*--Border--*/
  border: 1px solid #c0c0c0;
  -webkit-border-radius: 4px;
  /* Android ≤ 1.6, iOS 1-3.2, Safari 3-4 */
  border-radius: 4px;
  margin:5px 0 0 10px;
  padding:12px;
}

div.movement_line, div.trend_movement_line {
  text-indent:-24px;
  margin-bottom:4px;
  margin-left:20px;
}

div.movement_line span {
  line-height:18px;
  margin-top:-8px;
  margin-left:4px;
}

div#snapshot_radio_buttons, div#trend_radio_buttons {
  padding:12px 8px;
}

div#snapshot_radio_button_custom, div#trend_radio_button_custom {
  display:inline-block;
  margin-left:6px;
}

div#snapshot_radio_button_all, div#trend_radio_button_all {
  display:inline-block;
}

div#snapshot_button_uncheck_all, div#trend_button_uncheck_all {
  display:inline-block;
  margin-left:6px;
}

