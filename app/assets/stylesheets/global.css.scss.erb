.formtastic {
  fieldset {
    border: none;
    border-bottom: 1px solid silver;
    padding: 15px;
  }
  ol {
    list-style: none;
    margin: 0;
    padding: 0;
  }
  label {
    display: block;
    font-weight: normal;
    margin-bottom: .25em;
  }
  li {
    margin-bottom: .5em;
  }
  input[type=text], input[type=password], textarea {
    border: 1px solid gray;
    margin: 0;
    padding: 4px;
  }
  input[type=text], input[type=password] {
    min-width: 200px;
  }
  textarea {
    min-width: 400px;
    height: 5em;
  }
  select {
    border: 1px solid gray;
    padding: 4px;
  }
}

.customform {
  margin-top: 15px;
  .sgroup {
    margin: 20px 0;
  }
  textarea, input, select {
    margin: 0;
  }
  textarea, input[type=text], input[type=password] {
    border: 1px solid silver;
    padding: 3px;
    font-size: 12px;
    width: 290px;
  }
  select {
    border: 1px solid silver;
    border-radius: 2px;
    padding: 2px;
  }
  input.largefield {
    font-size: 16px;
  }
  .ls, .rs {
    position: relative;
    display: inline-block;
    vertical-align: top;
    line-height: 1.5em;
  }
  .ls {
    min-width: 100px;
    text-align: right;
    margin-right: 10px;
    label {
      font-weight: bold;
    }
    .tiplight {
      margin-right: 0px;
    }
  }
  .location {
    height: 60px;
  }
  .sfield {
    margin-bottom: 8px;
  }
  .sidebyside {
    .element {
      display: inline-block;
      text-align: center;
      margin-right: 10px;
      vertical-align: top;
    }
  }
  .leftalign .element {
    text-align: left;
  }
  .field_zip[type=text] {
    width: 70px;
  }
}

