{
  "name": "missionhub",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint app/assets/javascripts/angular app/assets/javascripts/test karma.conf.js",
    "lint-watch": "esw --watch app/assets/javascripts/angular app/assets/javascripts/test karma.conf.js",
    "test": "npm run lint && npm run test-once",
    "test-once": "karma start --single-run",
    "test-watch": "karma start",
    "test-debug": "karma start --browsers Chrome --reporters spec"
  },
  "dependencies": {
