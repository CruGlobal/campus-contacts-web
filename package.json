{
    "name": "missionhub",
    "version": "1.0.0",
    "scripts": {
        "start": "webpack-dev-server",
        "start-rails-api": "cd ../missionhub-api && rails s",
        "build": "webpack -p",
        "build:analyze": "webpack -p --env.analyze",
        "gql:schema": "apollo client:download-schema",
        "gql:codegen": "apollo client:codegen --target typescript",
        "test": "karma start --single-run",
        "test:cypress": "cypress run",
        "test:cypress:open": "run cypress open",
        "test:cypress:ci": "DEBUG=start-server-and-test START_SERVER_AND_TEST_INSECURE=0 start-server-and-test start https-get://localhost:8080 test:cypress",
        "test:react": "jest",
        "test:react:watch": "jest --watch",
        "test:react:coverage": "jest --coverage",
        "test:react:snapshot": "jest -u",
        "test:react:changes": "jest -o",
        "test-watch": "karma start",
        "test-debug": "karma start --browsers Chrome",
        "lint": "eslint './*.{js,ts,tsx}' './{src,app}/**/*.{js,ts,tsx}'",
        "lint:ts": "tsc",
        "lint:fix": "eslint './*.{js,ts,tsx}' './{src,app}/**/*.{js,ts,tsx}' --fix",
        "prettier:check": "prettier '{{src,app,bin,test,__{tests,mocks}__}/**/*.{js,json,html},./*.{js,json,html,yml}}' --list-different",
        "prettier:write": "prettier '{{src,app,bin,test,__{tests,mocks}__}/**/*.{js,json,html},./*.{js,json,html,yml}}' --write",
        "precommit": "pretty-quick --staged",
        "onesky:upload": "babel-node ./bin/oneskyUpload.js",
        "onesky:download": "babel-node ./bin/oneskyDownload.js"
    },
