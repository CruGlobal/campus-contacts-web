        "angularjs-scroll-glue": "^2.2.0",
        "angularjs-toaster": "^3.0.0",
        "angulartics": "^1.6.0",
        "angulartics-google-analytics": "^0.5.0",
        "bootstrap": "4.5.0",
        "countrily-data": "^1.0.1",
        "country-region-data": "^1.6.0",
        "i18next": "^19.4.5",
        "iso-639-1": "^2.1.3",
        "jsonapi-datastore": "^0.4.0-beta",
        "lodash": "^4.17.13",
        "m49-regions": "^1.2.0",
